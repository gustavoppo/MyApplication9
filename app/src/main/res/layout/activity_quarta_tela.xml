<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sem"
    tools:context="com.example.gusta.myapplication.QuartaTela"
    tools:layout_editor_absoluteY="81dp">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="68dp"
                android:layout_height="53dp"
                android:layout_above="@+id/radioGroup2"
                android:layout_alignLeft="@+id/radioGroup2"
                android:layout_alignStart="@+id/radioGroup2"
                android:layout_weight="1"
                app:srcCompat="@drawable/zx"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="21dp"
                tools:layout_editor_absoluteY="12dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_above="@+id/radioGroup2"
                android:layout_centerHorizontal="true"
                android:layout_weight="1"
                android:text="@string/qssl"
                android:textColor="@color/botcol"
                android:textSize="20sp"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="22dp" />

            <Button
                android:id="@+id/next"
                android:layout_width="wrap_content"
                android:layout_height="51dp"
                android:layout_above="@+id/top1"
                android:layout_toEndOf="@+id/textView6"
                android:layout_toRightOf="@+id/textView6"
                android:layout_weight="1"
                android:drawableTint="@color/botcol"
                android:text="@string/next"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="306dp"
                tools:layout_editor_absoluteY="12dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <RadioGroup
                android:id="@+id/radioGroup3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_weight="1"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="22dp"
                tools:layout_editor_absoluteY="65dp">

                <RadioButton
                    android:id="@+id/top1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:buttonTint="@color/bcol"
                    android:text="@string/top"
                    android:textColor="@color/botcol" />

                <RadioButton
                    android:id="@+id/jg1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:buttonTint="@color/bcol"
                    android:text="@string/jg"
                    android:textColor="@color/botcol" />

                <RadioButton
                    android:id="@+id/mid1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:buttonTint="@color/bcol"
                    android:text="@string/Mid"
                    android:textColor="@color/botcol" />

                <RadioButton
                    android:id="@+id/adc1"
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:buttonTint="@color/bcol"
                    android:text="@string/adc"
                    android:textColor="@color/botcol" />

                <RadioButton
                    android:id="@+id/sup1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:buttonTint="@color/bcol"
                    android:text="@string/sup"
                    android:textColor="@color/botcol" />

            </RadioGroup>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/top1"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_alignLeft="@+id/next"
                    android:layout_alignStart="@+id/next"
                    android:layout_below="@+id/textView6"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/top"
                    tools:ignore="DuplicateIds,MissingConstraints"
                    tools:layout_editor_absoluteX="259dp"
                    tools:layout_editor_absoluteY="79dp" />

                <ImageView
                    android:id="@+id/jg1"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_alignLeft="@+id/top1"
                    android:layout_alignStart="@+id/top1"
                    android:layout_below="@+id/top1"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/jg"
                    tools:ignore="DuplicateIds,MissingConstraints"
                    tools:layout_editor_absoluteX="259dp"
                    tools:layout_editor_absoluteY="155dp" />

                <ImageView
                    android:id="@+id/mid1"
                    android:layout_width="match_parent"
                    android:layout_height="63dp"
                    android:layout_alignLeft="@+id/jg1"
                    android:layout_alignStart="@+id/jg1"
                    android:layout_centerVertical="true"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/mid"
                    tools:ignore="DuplicateIds,MissingConstraints"
                    tools:layout_editor_absoluteX="259dp"
                    tools:layout_editor_absoluteY="235dp" />

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_above="@+id/sup1"
                    android:layout_alignLeft="@+id/mid1"
                    android:layout_alignStart="@+id/mid1"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/bot"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="259dp"
                    tools:layout_editor_absoluteY="314dp" />

                <ImageView
                    android:id="@+id/sup1"
                    android:layout_width="match_parent"
                    android:layout_height="53dp"
                    android:layout_alignBottom="@+id/radioGroup2"
                    android:layout_alignLeft="@+id/mid1"
                    android:layout_alignStart="@+id/mid1"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/sop"
                    tools:ignore="DuplicateIds,MissingConstraints"
                    tools:layout_editor_absoluteX="293dp"
                    tools:layout_editor_absoluteY="404dp" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
