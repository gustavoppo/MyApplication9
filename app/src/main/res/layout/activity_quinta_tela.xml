<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gusta.myapplication.QuintaTela"
    android:background="@drawable/sem">

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:layout_editor_absoluteY="9dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="7dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView21"
            android:layout_marginBottom="27dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_toEndOf="@+id/imageView21"
            android:layout_toRightOf="@+id/imageView21"
            android:text="@string/nsm"
            android:textSize="20sp"
            android:textColor="@color/bcola"/>

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="73dp"
            android:layout_height="73dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/textView14"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="53dp"
            app:srcCompat="@drawable/red" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView21"
            android:layout_alignLeft="@+id/textView5"
            android:layout_alignStart="@+id/textView5"
            android:layout_marginBottom="11dp"
            android:text="Level 30   "
            android:textColor="@color/botcol"/>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView7"
            android:layout_alignBottom="@+id/textView7"
            android:layout_toEndOf="@+id/textView7"
            android:layout_toRightOf="@+id/textView7"
            android:text="|  Br"
            android:textColor="@color/botcol"/>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/textView14"
            android:layout_toStartOf="@+id/textView14"
            android:text="@string/suala"
            android:textColor="@color/botcol"/>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView13"
            android:layout_alignRight="@+id/textView13"
            android:layout_centerVertical="true"
            android:text="@string/suald"
            android:textColor="@color/botcol"/>

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="63dp"
            android:layout_height="63dp"
            android:layout_alignBottom="@+id/imageView21"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="11dp"
            android:layout_marginRight="11dp"
            app:srcCompat="@drawable/gold" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView22"
            android:layout_alignRight="@+id/imageView22"
            android:layout_below="@+id/imageView22"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            android:text="@string/ouro"
            android:textSize="15sp"
            android:textColor="@color/bcola"/>

        <ImageView
            android:id="@+id/imageView23"
            android:layout_width="63dp"
            android:layout_height="63dp"
            android:layout_alignEnd="@+id/textView11"
            android:layout_alignRight="@+id/textView11"
            android:layout_below="@+id/textView11"
            android:layout_marginEnd="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="17dp"
            app:srcCompat="@drawable/bot" />

        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="63dp"
            android:layout_height="63dp"
            android:layout_alignTop="@+id/imageView23"
            android:layout_toLeftOf="@+id/textView13"
            android:layout_toStartOf="@+id/textView13"
            app:srcCompat="@drawable/mid" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="98dp"
            android:layout_height="53dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="13dp"
            android:text="@string/per"
            android:textSize="30sp"
            android:textColor="@color/bcol"
            />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="53dp"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView23"
            android:layout_alignRight="@+id/imageView23"
            android:layout_below="@+id/imageView23"
            android:text="@string/adc"
            android:textColor="@color/bcol"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Mid"
            android:layout_below="@+id/imageView24"
            android:layout_toLeftOf="@+id/imageView22"
            android:layout_toStartOf="@+id/imageView22"
            android:textColor="@color/bcol"/>

        <CheckedTextView
            android:id="@+id/checkedTextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="58dp"
            android:text="@string/edper"
            android:onClick="editar"
            android:textColor="@color/cinza"
            android:layout_below="@+id/textView15"
            android:layout_alignRight="@+id/textView15"
            android:layout_alignEnd="@+id/textView15" />

        <CheckedTextView
            android:id="@+id/checkedTextView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/d"
            android:textColor="@color/cinza"
            android:layout_alignBaseline="@+id/checkedTextView3"
            android:layout_alignBottom="@+id/checkedTextView3"
            android:layout_alignRight="@+id/textView16"
            android:layout_alignEnd="@+id/textView16" />

        <Button
            android:id="@+id/next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/buscar"
            android:layout_alignTop="@+id/textView14"
            android:layout_alignLeft="@+id/textView16"
            android:layout_alignStart="@+id/textView16" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
