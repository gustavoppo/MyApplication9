<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sem"
    tools:context="com.example.gusta.myapplication.TerceiraTela"
    tools:layout_editor_absoluteY="81dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="53dp"
                android:layout_height="53dp"
                android:layout_alignLeft="@+id/radioGroup2"
                android:layout_alignStart="@+id/radioGroup2"
                android:layout_alignTop="@+id/textView6"
                android:layout_weight="1"
                app:srcCompat="@drawable/zx"
                tools:layout_editor_absoluteX="21dp"
                tools:layout_editor_absoluteY="13dp" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_weight="1"
                android:text="@string/qsl"
                android:textColor="@color/bcol"
                android:textSize="25sp"
                tools:layout_editor_absoluteX="112dp"
                tools:layout_editor_absoluteY="13dp" />

            <Button
                android:id="@+id/next"
                android:layout_width="65dp"
                android:layout_height="53dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignTop="@+id/textView6"
                android:layout_weight="1"
                android:drawableTint="@color/botcol"
                android:text="@string/next"
                tools:layout_editor_absoluteX="302dp"
                tools:layout_editor_absoluteY="13dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <RadioGroup
                    android:id="@+id/radioGroup2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignTop="@+id/top1"
                    android:layout_marginBottom="16dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="29dp"
                    android:layout_weight="1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView16"
                    app:layout_constraintVertical_bias="0.0">

                    <RadioButton
                        android:id="@+id/radiotop"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="20dp"
                        android:layout_weight="0"
                        android:buttonTint="@color/bcol"
                        android:text="@string/top"
                        android:textColor="@color/botcol"
                        tools:targetApi="lollipop" />

                    <RadioButton
                        android:id="@+id/radiojg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/bcol"
                        android:text="@string/jg"
                        android:textColor="@color/botcol" />

                    <RadioButton
                        android:id="@+id/radiomid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/bcol"
                        android:text="@string/Mid"
                        android:textColor="@color/botcol" />

                    <RadioButton
                        android:id="@+id/radioadc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/bcol"
                        android:text="@string/adc"
                        android:textColor="@color/botcol" />

                    <RadioButton
                        android:id="@+id/radiosup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/bcol"
                        android:text="@string/sup"
                        android:textColor="@color/botcol" />
                </RadioGroup>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/top1"
                        android:layout_width="match_parent"
                        android:layout_height="53dp"
                        android:layout_above="@+id/jg1"
                        android:layout_alignLeft="@+id/jg1"
                        android:layout_alignStart="@+id/jg1"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/top"
                        tools:layout_editor_absoluteX="272dp"
                        tools:layout_editor_absoluteY="88dp" />

                    <ImageView
                        android:id="@+id/jg1"
                        android:layout_width="match_parent"
                        android:layout_height="53dp"
                        android:layout_above="@+id/mid1"
                        android:layout_toEndOf="@+id/radioGroup2"
                        android:layout_toRightOf="@+id/radioGroup2"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/jg"
                        tools:layout_editor_absoluteX="272dp"
                        tools:layout_editor_absoluteY="161dp" />

                    <ImageView
                        android:id="@+id/mid1"
                        android:layout_width="match_parent"
                        android:layout_height="54dp"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/mid"
                        tools:layout_editor_absoluteX="272dp"
                        tools:layout_editor_absoluteY="239dp" />

                    <ImageView
                        android:id="@+id/adc1"
                        android:layout_width="match_parent"
                        android:layout_height="53dp"
                        android:layout_above="@+id/sup1"
                        android:layout_alignLeft="@+id/sup1"
                        android:layout_alignStart="@+id/sup1"
                        android:layout_weight="1"
                        android:contentDescription="TODO"
                        app:srcCompat="@drawable/bot"
                        tools:layout_editor_absoluteX="272dp"
                        tools:layout_editor_absoluteY="329dp" />

                    <ImageView
                        android:id="@+id/sup1"
                        android:layout_width="match_parent"
                        android:layout_height="53dp"
                        android:layout_alignBottom="@+id/radioGroup2"
                        android:layout_alignLeft="@+id/mid1"
                        android:layout_alignStart="@+id/mid1"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/sop"
                        tools:layout_editor_absoluteX="272dp"
                        tools:layout_editor_absoluteY="414dp" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
